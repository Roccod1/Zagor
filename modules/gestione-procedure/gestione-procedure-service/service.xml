<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.4.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_4_0.dtd">

<service-builder dependency-injector="ds" package-path="it.servizidigitali.gestioneprocedure" auto-namespace-tables="false">
	<namespace/>
	
	<entity 
		name="Procedura"
		table="procedura"
		uuid="true"
		local-service="true" 
		remote-service="false" 
		data-source="servizidigitaliDataSource" 
		cache-enabled="true"
		>

		<!-- PK fields -->
		<column name="proceduraId" primary="true" type="long" />

		<!-- Group instance -->
		<column name="groupId" type="long" />

		<!-- Audit fields -->
		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->
		<column name="nome" type="String" />
		<column name="pecDestinazione" type="String" />
		<column name="step1Attivo" type="boolean" />
		<column name="step1TipoComponentiNucleoFamiliare" type="String" />
		<column name="step2TipoServizio" type="String" />
		<column name="step2TipiIntegrazioneBackoffice" type="String" />
		<column name="abilitaCacheIntegrazioneBackoffice" type="boolean" />
		<column name="tipoGenerazionePDF" type="String" />
		<column name="attiva" type="boolean" />
		
		<column name="servizioId" type="long" />
		<column name="processoId" type="long" />
		
		<!-- Finder methods -->
		<finder name="Attiva" return-type="Collection">
			<finder-column name="attiva" />
		</finder>

		<finder name="GroupIdServizioIdProcessoId" return-type="Collection">
			<finder-column name="groupId" />
			<finder-column name="servizioId" />
			<finder-column name="processoId" />
		</finder>

		<finder name="GroupIdServizioId" return-type="Collection">
			<finder-column name="groupId" />
			<finder-column name="servizioId" />
		</finder>
		
		<finder name="GroupIdServizioIdAttiva" return-type="Collection">
			<finder-column name="groupId" />
			<finder-column name="servizioId" />
			<finder-column name="attiva" />
		</finder>
		
		<finder name="GroupIdAttiva" return-type="Collection">
			<finder-column name="groupId" />
			<finder-column name="attiva" />
		</finder>
		
	</entity>

	<entity 
		name="ProceduraForm"
		table="procedura_form"
		uuid="true"
		local-service="true" 
		remote-service="false" 
		data-source="servizidigitaliDataSource" 
		cache-enabled="true"
		>

		<!-- PK fields -->
		<column name="proceduraId" primary="true" type="long" />
		<column name="formId" primary="true" type="long" />

		<!-- Audit fields -->
		<column name="companyId" type="long" />
		
        <finder name="ProceduraId" return-type="Collection">
            <finder-column name="proceduraId"/>
        </finder>
	</entity>
	
	<entity 
		name="TemplatePdf"
		table="template_pdf"
		uuid="true"
		local-service="true" 
		remote-service="false" 
		data-source="servizidigitaliDataSource" 
		cache-enabled="true"
		>

		<!-- PK fields -->
		<column name="templatePdfId" primary="true" type="long" />
		
		<!-- Group instance -->
		<column name="groupId" type="long" />
		
		<!-- Audit fields -->
		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />
		
		<column name="fileEntryId" type="long" />
		<column name="proceduraId" type="long" />
		<column name="templatePdfParentId" type="long" convert-null="false"/>

        <finder name="TemplatePdfParentId" return-type="Collection">
            <finder-column name="templatePdfParentId"/>
        </finder>
        
        <finder name="ProceduraId" return-type="Collection">
            <finder-column name="proceduraId"></finder-column>
        </finder>
        
	</entity>
</service-builder>