<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.4.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_4_0.dtd">

<service-builder dependency-injector="ds" package-path="it.servizidigitali.gestioneenti" auto-namespace-tables="false">
	<namespace/>
	
	<entity 
		name="ServizioEnte"
		table="servizio_ente"
		uuid="true"
		local-service="true" 
		remote-service="false" 
		data-source="servizidigitaliDataSource" 
		cache-enabled="true"
		>

		<!-- PK fields -->
		<column name="servizioId" primary="true" type="long" />
		<column name="organizationId" primary="true" type="long" />

		<!-- Group instance -->
		<column name="groupId" type="long" />

		<!-- Audit fields -->
		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->
		<column name="uri" type="String" />
		<column name="uriGuest" type="String" />
		<column name="uriScheda" type="String" />
		<column name="autenticazione" type="boolean" />
		<column name="dataInizioAttivazione" type="Date" />
		<column name="dataFineAttivazione" type="Date" />
		<column name="cittadino" type="boolean" />
		<column name="azienda" type="boolean" />
		<column name="delega" type="boolean" />
		<column name="chatbot" type="boolean" />
		<column name="prenotabile" type="boolean" />
		<column name="privacyDelega" type="boolean" />
		<column name="allegatoDelega" type="boolean" />
		<column name="timbroCertificato" type="boolean" />
		<column name="iseeInps" type="boolean" />
		<column name="attivo" type="boolean" />
		
		<!-- Finder methods -->
		<finder name="OrganizationId" return-type="Collection">
			<finder-column name="organizationId" />
		</finder>

		<finder name="ServizioId" return-type="Collection">
			<finder-column name="servizioId" />
		</finder>
	</entity>
</service-builder>