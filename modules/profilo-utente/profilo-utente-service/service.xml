<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.4.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_4_0.dtd">

<service-builder dependency-injector="ds" package-path="it.servizidigitali.profiloutente" auto-namespace-tables="false">
	<namespace/>
	
	<entity 
		name="UtenteOrganizzazione"
		table="utente_organizzazione"
		uuid="true"
		local-service="true" 
		remote-service="false" 
		data-source="servizidigitaliDataSource" 
		cache-enabled="true"
		>

		<!-- PK fields -->
		<column name="utenteId" primary="true" type="long" />
		<column name="organizationId" primary="true" type="long" />

		<!-- Group instance -->
		<column name="groupId" type="long" />

		<!-- Audit fields -->
		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->
		<column name="preferito" type="boolean" />
		
		<!-- Finder methods -->
		<finder name="OrganizationId" return-type="Collection">
			<finder-column name="organizationId" />
		</finder>

		<finder name="UtenteId" return-type="Collection">
			<finder-column name="utenteId" />
		</finder>
		
		<finder name="O_U_P" return-type="Collection">
			<finder-column name="organizationId" />
			<finder-column name="utenteId" />
			<finder-column name="preferito" />
		</finder>
		
		<finder name="U_P" return-type="Collection">
			<finder-column name="utenteId" />
			<finder-column name="preferito" />
		</finder>
	</entity>
	
	<entity 
		name="CanaleComunicazione"
		table="canale_comunicazione"
		uuid="false"
		local-service="true" 
		remote-service="false" 
		data-source="servizidigitaliDataSource" 
		cache-enabled="true"
		>

		<!-- PK fields -->
		<column name="canaleComunicazioneId" primary="true" type="long" />

		<!-- Other fields -->
		<column name="nome" type="String" />
		<column name="codice" type="String" />
		<column name="attivo" type="boolean" />
		
		<!-- Finder methods -->
		<finder name="Codice" return-type="CanaleComunicazione">
			<finder-column name="codice" />
		</finder>

		<finder name="Attivo" return-type="Collection">
			<finder-column name="attivo" />
		</finder>
	</entity>

	<entity 
		name="UtenteOrganizzazioneCanaleComunicazione"
		table="utente_organizzazione_canalecomunicazione"
		uuid="true"
		local-service="true" 
		remote-service="false" 
		data-source="servizidigitaliDataSource" 
		cache-enabled="true"
		>

		<!-- PK fields -->
		<column name="utenteId" primary="true" type="long" />
		<column name="organizationId" primary="true" type="long" />
		<column name="canaleComunicazioneId" primary="true" type="long" />

		<!-- Group instance -->
		<column name="groupId" type="long" />

		<!-- Audit fields -->
		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />
		
		<!-- Finder methods -->
		<finder name="OrganizationId" return-type="Collection">
			<finder-column name="organizationId" />
		</finder>

		<finder name="UtenteId" return-type="Collection">
			<finder-column name="utenteId" />
		</finder>
		
		<finder name="UtenteOrganization" return-type="Collection">
			<finder-column name="utenteId"/>
			<finder-column name="organizationId" />
		</finder>
	</entity>
</service-builder>